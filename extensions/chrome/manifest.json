{
  "manifest_version": 2,
  "name": "fi",
  "description": "A tool which gives you a quick look at `font-family` usage in a webpage.",
  "version": "1.1.0",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "128": "128.png"
    }
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }
}